{
  "name": "fast-formula-parser",
  "version": "1.0.19",
  "description": "fast excel formula parser",
  "repository": "https://github.com/LesterLyu/fast-formula-parser",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "test": "mocha -s 0",
    "test:f": "mocha test/formulas",
    "prepublishOnly": "yarn run build && yarn run test",
    "postpublish": "yarn run publish:docs",
    "build:node": "babel src -d lib --source-maps",
    "build:umd": "webpack",
    "build": "yarn build:node && yarn build:umd",
    "size": "source-map-explorer build/parser.min.js*",
    "diagram": "node lib/grammar/diagram.js",
    "docs": "yarn run diagram && jsdoc --configure .jsdoc.json --verbose",
    "publish:docs": "yarn run docs && yarn run coverage && gh-pages -d docs",
    "coverage": "nyc -x lib/ssf -x test --reporter=html --reporter=text --report-dir=docs/coverage mocha",
    "coverage:f": "nyc -x lib/grammar -x lib/ssf -x test --reporter=html --reporter=text mocha test/formulas",
    "coveralls": "nyc -x lib/ssf -x test --reporter=lcovonly --report-dir=coverage mocha"
  },
  "keywords": [
    "excel",
    "formula",
    "spreadsheet",
    "javascript",
    "js",
    "parser",
    "excel-formula"
  ],
  "author": "Lester(Dishu) Lyu",
  "license": "MIT",
  "dependencies": {
    "bahttext": "^2.0.2",
    "bessel": "^1.0.2",
    "chevrotain": "^7.0.3",
    "jstat-esm": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "chai": "^4.2.0",
    "docdash": "^1.2.0",
    "gh-pages": "^3.1.0",
    "jsdoc": "^3.6.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "source-map-explorer": "^2.5.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}
